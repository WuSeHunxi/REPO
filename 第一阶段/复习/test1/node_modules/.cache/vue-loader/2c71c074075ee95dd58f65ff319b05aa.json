r.push({\n                    type:'prev',\n                    title:prev,\n                    id:prevId\n                })\n            }\n\n            if(this.question.next){\n                const {next,nextId}=this.question;\n                arr.push({\n                    type:'next',\n                    title:next,\n                    id:nextId\n                })\n            }\n            return arr;\n        }\n    },\n    methods:{\n        handleClick(id){\n            // this.getData() \n            const {name}=this.$route;\n            console.log(id)\n            this.$router.push({\n                name,\n                params:{id}\n            })\n        },\n        getData(){\n            const {id}=this.$route.params;\n\n            this.$axios.get(`/question/${id}`).then(res=>{\n                this.question=res;\n                console.log(res)\n            })\n        }\n    },\n    watch:{\n        '$route'(){\n            this.getData()\n        }\n    }\n}\n</script>\n\n<style scoped>\n.main {\n  margin-bottom: 200px;\n}\n\n.prev {\n  float: left;\n}\n\n.next {\n  float: right;\n}\n\n.prev,\n.next {\n  width: 200px;\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  color: #3385ff;\n  text-decoration: underline;\n  cursor: pointer;\n}\n</style>"]}]}